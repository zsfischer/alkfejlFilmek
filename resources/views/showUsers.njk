{% extends "layout" %}

{% block scripts %}
<script src="/scripts/logout.js"></script>
{% endblock %}

{% block content %}
<h2>Felhasználók</h2>

<table class="table table-striped table-hover ">
  <thead>
    <tr class="warning">
      <th>ID</th>
      <th>Felhasználónév</th>
      <th>Email cím</th>
      <th>Regisztráció időpontja</th>
    </tr>
  </thead>
  <tbody>
  {% for user in users %}
    <tr>
      <td>{{user.id}}</td>
      <td>{{user.username}}</td>
      <td>{{user.email}}</td>
      <td>{{user.created_at}}</td>
    </tr>
    {% endfor %}
  </tbody>
</table>

{% endblock %}